<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1.1.0" id="com.Currency.Converter" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="mdpi" src="www/img/iconoapp.png" />
    <name>Currency Converter</name>
    <description>
       App para convertir divisas.
    </description>
    <author email="verardomatias@gmail.com" href="http://uberall.la">
        Uberall Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
